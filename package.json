{
  "name": "static-express-server",
  "version": "1.0.0",
  "description": "simple express server to deploy static JS frontend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/index.ts --dev'",
    "build": " tsc -b",
    "watch": "tsc -w",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dron84/static-express-server.git"
  },
  "keywords": [
    "static",
    "express",
    "server"
  ],
  "author": "Dron84@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dron84/static-express-server/issues"
  },
  "homepage": "https://github.com/Dron84/static-express-server#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15"
  }
}
